{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "groups",
        "total"
    ],
    "properties": {
        "total": {
            "type": "integer"
        },
        "groups": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "name",
                    "createdAt",
                    "createdBy",
                    "completedAt",
                    "progress",
                    "status",
                    "mainSourceFiles",
                    "numAnalyses",
                    "numVulnerabilities"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string"
                    },
                    "createdBy": {
                        "type": "string"
                    },
                    "completedAt": {
                        "type": ["string", "null"]
                    },
                    "progress": {
                        "type": "number"
                    },
                    "status": {
                        "type": "string",
                        "enum": [
                            "opened",
                            "sealed"
                        ]
                    },
                    "mainSourceFiles": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "numAnalyses": {
                        "type": "object",
                        "required": [
                            "total",
                            "queued",
                            "running",
                            "failed",
                            "finished"
                        ],
                        "properties": {
                            "total": {
                                "type": "integer"
                            },
                            "queued": {
                                "type": "integer"
                            },
                            "running": {
                                "type": "integer"
                            },
                            "failed": {
                                "type": "integer"
                            },
                            "finished": {
                                "type": "integer"
                            }
                        }
                    },
                    "numVulnerabilities": {
                        "type": "object",
                        "required": [
                            "high",
                            "medium",
                            "low"
                        ],
                        "properties": {
                            "high": {
                                "type": "integer"
                            },
                            "medium": {
                                "type": "integer"
                            },
                            "low": {
                                "type": "integer"
                            }
                        }
                    }
                }
            }
        }
    }
}
